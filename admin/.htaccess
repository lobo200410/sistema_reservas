# Deshabilitar listado de directorios
Options -Indexes

# Si entran directo a /admin/ cargar dashboard.php
DirectoryIndex dashboard.php

# Asegurar que siempre redirija a dashboard.php
RewriteEngine On
RewriteCond %{REQUEST_URI} ^/reservas/admin/?$ [NC]
RewriteRule ^.*$ /reservas/admin/dashboard.php [L,R=302]
